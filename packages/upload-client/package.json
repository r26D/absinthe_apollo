{
  "name": "@r26d/absinthe-apollo-upload-client",
  "version": "0.0.2-alpha.0",
  "description": "A terminating Apollo Link for Apollo Client that allows FileList, File, Blob or ReactNativeFile instances within query or mutation variables and sends GraphQL multipart requests.",
  "license": "MIT",
  "contributors": [
    {
      "name": "Jayden Seric",
      "email": "me@jaydenseric.com",
      "url": "https://jaydenseric.com"
    },
    {
      "name": "Martin Richter",
      "url": "http://www.martinrichter.net/"
    },
    {
      "name": "Dirk Elmendorf",
      "url": "http://www.r26d.com"
    }
  ],
  "repository": "github:r26D/absinthe_apollo",
  "homepage": "https://github.com/r26D/absinthe_apollo#readme",
  "bugs": "https://github.com/r26D/absinthe_apollo/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "graphql",
    "multipart",
    "request",
    "file",
    "upload",
    "apollo",
    "client",
    "link",
    "react",
    "native"
